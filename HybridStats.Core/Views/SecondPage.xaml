<?xml version="1.0" encoding="utf-8" ?>
<views:BasePage  xmlns="http://xamarin.com/schemas/2014/forms"
                 xmlns:views="clr-namespace:HybridStats.Core.Views"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:ViewModels="clr-namespace:HybridStats.Core.ViewModels"
                 x:TypeArguments="ViewModels:SecondViewModel"
                 x:Class="HybridStats.Core.Views.SecondPage"
                 BackgroundColor="LightSalmon">
    <ContentPage.Content>
        <Grid>
            <Label Text="Welcome to Xamarin Forms!" FontSize="Large" HorizontalOptions="Center" Margin="50"/>
            <Button Text="Next" Command="{Binding NextPageCommand}" HorizontalOptions="Center" VerticalOptions="Center"/>
        </Grid>
    </ContentPage.Content>
</views:BasePage>